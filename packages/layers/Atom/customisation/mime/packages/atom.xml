     <glob pattern="*.csv"/>
     <glob pattern="*.h"/>
     <glob pattern="*.hpp"/>
     <glob pattern="*.html"/>
     <glob pattern="*.java"/>
     <glob pattern="*.js"/>
     <glob pattern="*.php"/>
     <glob pattern="*.py"/>
     <glob pattern="*.sql"/>
     <glob pattern="*.tsv"/>
     <glob pattern="*.txt"/>
     <glob pattern="*.xml"/>
   </mime-type>
</mime-info>
